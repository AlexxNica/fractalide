  name = genName ./.;
  src = ./.;
  filteredContracts = filterContracts [ "net_ndn_interest" ];
  depsSha256 = "0fn4cxc8z8yhi6s38na4d86khy74wnfy9sgah3kn5vaf5dm870rz";
